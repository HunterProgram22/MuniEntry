# -*- mode: python ; coding: utf-8 -*-


block_cipher = None


a = Analysis(['MuniEntry_app.py'],
             pathex=[],
             binaries=[],
             datas=[('./config.ini', '.'),
                    ('./db/MuniEntryDB.sqlite', './db'),
                    ('./db/Arraignments.xlsx', './db'),
                    ('./db/Batch_FTA_Arraignments.xlsx', './db'),
                    ('./db/Slated.xlsx', './db'),
                    ('./db/Final_Pretrials.xlsx', './db'),
                    ('./db/Pleas.xlsx', './db'),
                    ('./db/PCVH_FCVH.xlsx', './db'),
                    ('./db/Trials_to_Court.xlsx', './db'),
                    ('./resources/Templates/Not_Guilty_Bond_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Driving_Privileges_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Batch_Failure_To_Appear_Arraignment_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Bond_Hearing_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Fine_Only_Plea_Final_Judgment_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Leap_Admission_Plea_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Leap_Sentencing_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Jail_Plea_Final_Judgment_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Diversion_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Failure_To_Appear_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Probation_Violation_Bond_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Plea_Only_Template.docx', './resources/Templates'),
                    ('./resources/Templates/No_Plea_Bond_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Trial_Sentencing_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Scheduling_Entry_Template_Rohrer.docx', './resources/Templates'),
                    ('./resources/Templates/Scheduling_Entry_Template_Hemmeter.docx', './resources/Templates'),
                    ('./resources/Templates/Trial_To_Court_Hearing_Notice_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Final_Jury_Notice_Of_Hearing_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Sentencing_Only_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Freeform_Entry_Template.docx', './resources/Templates'),
                    ('./resources/Templates/Leap_Admission_Plea_Already_Valid_Template.docx', './resources/Templates'),
                    ('./resources/Templates/General_Notice_Of_Hearing_Template.docx', './resources/Templates'),
                    ('./resources/Saved/holder.txt', './resources/Saved'),
                    ('./resources/Saved/batch/holder.txt', './resources/Saved/batch'),
                    ('./resources/icons/gavel_main_splash.png', './resources/icons'),
                    ('./resources/icons/gavel_splash.png', './resources/icons'),
                    ('./resources/icons/gavel.ico', './resources/icons'),
                    ],
             hiddenimports=['lxml._elementpath'],
             hookspath=[],
             hooksconfig={},
             runtime_hooks=[],
             excludes=['pytest', 'pytest-qt', './tests', './stubs'],
             win_no_prefer_redirects=False,
             win_private_assemblies=False,
             cipher=block_cipher,
             noarchive=False)
pyz = PYZ(a.pure, a.zipped_data,
             cipher=block_cipher)
splash = Splash(
                './resources/icons/gavel_splash.png',
                binaries=a.binaries,
                datas=a.datas,
                text_pos=None,
                text_size=12,
                minify_script=True)

exe = EXE(pyz,
          a.scripts,
          splash,
          [],
          exclude_binaries=True,
          name='MuniEntry_app',
          debug=False,
          bootloader_ignore_signals=False,
          strip=False,
          icon='./resources/icons/gavel.ico',
          upx=True,
          console=False,
          disable_windowed_traceback=False,
          target_arch=None,
          codesign_identity=None,
          entitlements_file=None )
coll = COLLECT(exe,
               a.binaries,
               a.zipfiles,
               a.datas,
               splash.binaries,
               strip=False,
               upx=True,
               upx_exclude=[],
               name='MuniEntry_app')
